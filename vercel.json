{
    "rewrites":[
        {
            "source":"/(.*)",
            "destination": "/index.html"
        }
    ],
    "headers":[
        {
            "source":"/service-worker.js",
            "headers":[
                {
                    "key":"Cache-control",
                    "value": "public, max-age=0, must-revalidate"
                }
            ]
        }
    ]
}